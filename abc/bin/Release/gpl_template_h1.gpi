set term png size 1920, 1080

set datafile separator ","

set xrange [1:5]
set yrange [1E-4:1]

set logscale x
set logscale y

set grid
set grid lw 1 lc rgb "black"

unset key

set xtics font ",18"
set ytics font ",18"

set xtics offset 0,-1.5

set ytics format "%.0E"
set xtics (2, 3, 4, 5, 6, 7, 8, 9, 10)

set title font ",18"

set dashtype 1 '.'
set dashtype 2 '-'
set dashtype 3 '_'

set pointsize 3

