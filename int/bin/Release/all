#!/bin/bash

./run-all

./combine_data_1 -d int -f ring -lb 12 -la 4 -p 1 -s table_PHI -a PHI
./combine_data_1 -d int -f ring -lb 12 -la 4 -p 1 -s table_E -a E
./combine_data_1 -d int -f ring -lb 12 -la 4 -p 1 -s table_D -a D

./combine_data_1 -d int -f shell -lb 12 -la 4 -p 1 -s table_PHI -a PHI
./combine_data_1 -d int -f shell -lb 12 -la 4 -p 1 -s table_E -a E
./combine_data_1 -d int -f shell -lb 12 -la 4 -p 1 -s table_D -a D

