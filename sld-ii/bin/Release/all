#!/bin/bash

./run-all

./combine_data_1 -d sld-ii -f square -lb 12 -la 4 -p 1 -s table_THETA -a THETA
./combine_data_1 -d sld-ii -f square -lb 12 -la 4 -p 1 -s table_H -a H

./combine_data_1 -d sld-ii -f cube -lb 12 -la 4 -p 1 -s table_THETA -a THETA
./combine_data_1 -d sld-ii -f cube -lb 12 -la 4 -p 1 -s table_H -a H

